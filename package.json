{
  "name": "zensqlite",
  "version": "1.8.0",
  "description": "Type safe tooling for SQLite",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/zensqlite#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/zensqlite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/zensqlite.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js"
    }
  },
  "main": "./dist/mod.js",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist deno_dist && tsup --format cjs,esm src/mod.ts --dts src/mod.ts && denoify",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:clear-cache": "jest --clearCache",
    "test:clear-cache:run": "jest --clearCache && jest",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "denoify": "^0.10.6",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.4.7",
    "jest-ts-tsx-resolver": "^1.0.0",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
