{
  "name": "zensqlite",
  "version": "2.0.0-7",
  "description": "Type safe tooling for SQLite",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/zensqlite#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/zensqlite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/zensqlite.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js"
    }
  },
  "main": "./dist/mod.js",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist deno_dist && tsup --format cjs,esm src/mod.ts --dts src/mod.ts && denoify",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:clear-cache": "jest --clearCache",
    "test:clear-cache:run": "jest --clearCache && jest",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit"
  },
  "dependencies": {
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@swc/core": "^1.3.62",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "dedent": "^0.7.0",
    "denoify": "^1.5.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "jest-ts-tsx-resolver": "^1.0.0",
    "np": "^8.0.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sql-formatter": "^12.2.1",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
